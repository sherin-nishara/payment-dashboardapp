{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    },
    "noEmit": true,
    "jsx": "react-native",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "skipLibCheck": true
  },
  "include": [
    "src",
    "App.tsx",
    "App.js",
    "expo-env.d.ts",
    ".expo/types/**/*.ts"
, "app/services", "app/components", "app/screens"  ],
  "exclude": ["node_modules"]
}